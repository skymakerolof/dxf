{
  "name": "dxf",
  "version": "5.0.0",
  "description": "DXF parser for node/browser",
  "main": "lib/index.js",
  "bin": {
    "dxf-to-svg": "lib/cli.js"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "compile": "babel -d lib/ src/",
    "dist": "browserify lib/index.js --standalone dxf -o dist/dxf.js",
    "lint": "eslint src test",
    "prepublishOnly": "npm run lint && npm run test && rimraf dist/ lib/ && npm run compile && npm run dist",
    "prettier": "prettier --write .",
    "test": "npm run test:unit",
    "test:functional": "cd test/functional && webpack-dev-server",
    "test:unit": "mocha --require @babel/register --recursive test/unit/**",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/skymakerolof/dxf.git"
  },
  "keywords": [
    "DXF",
    "Parser",
    "SVG"
  ],
  "author": "Ben Nortier <ben@bjnortier.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skymakerolof/dxf/issues"
  },
  "homepage": "https://github.com/skymakerolof/dxf",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "browserify": "^16.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-standard": "^4.1.0",
    "expect": "^24.9.0",
    "mocha": "^10.2.0",
    "open": "^7.4.2",
    "prettier": "^2.8.0",
    "raw-loader": "^1.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.3.4",
    "rimraf": "^2.7.1",
    "standard-version": "^9.5.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.3",
    "xml2js": "^0.4.23"
  },
  "dependencies": {
    "commander": "^2.20.3",
    "lodash": "^4.17.15",
    "vecks": "^3.9.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after"
    ],
    "ignore": [
      "lib/",
      "dist/"
    ]
  }
}
